@Library('shared-libraries') _
pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script{
                    functions.build(filePath: "filepath"
                                    DockerImage: "some docker image"
                    )
                }
            }
        }
        stage('docker push') {
            steps {
                script{
                    functions.push(DockerImage: "some docker image")
                }
            }
        }
    }
}